def visualize_cell_features(cells_df, features=['num_punctas', 'cell_volume_voxels']):
    """
    Create histograms and boxplots for selected features, split by condition.
    """
    for feat in features:
        if feat not in cells_df.columns:
            continue
        fig, axes = plt.subplots(1, 3, figsize=(15, 5))

        # Histograms
        sns.histplot(
            data=cells_df, x=feat, hue='condition', kde=True, palette=custom_palette,
            hue_order=['control', 'sick'], stat='proportion', common_norm=False, ax=axes[1]
        )
        axes[1].set_title(f'Distribution of {feat} by condition')
        sns.histplot(
            data=cells_df, x=feat, hue='cell_type', kde=True,
            stat='proportion', common_norm=False, ax=axes[0]
        )        
        axes[0].set_title(f'Distribution of {feat} by cell type')

        # Boxplot
        sns.violinplot(
            data=cells_df, x='cell_type', y=feat, hue='condition', palette=custom_palette,
            hue_order=['control', 'sick'], ax=axes[2]
        )
        axes[2].set_title(f'{feat} by Condition')

        plt.tight_layout()
        plt.show()



